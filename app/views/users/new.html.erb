<% provide(:title, 'Sign up') %>
<div class="loginWrapper">
  <div class="loginForm">
    <h1>Sign up</h1>
    <%= form_for(@user) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'shared/user_form', f: f %>
      <%= f.submit "Create my account", class: "btn btn-large btn-login" %>
    <% end %>
  </div>
</div>